{
  "name": "illusive-man/converter",
  "description": "Convertes number to its text representation",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage":  "https://github.com/illusive-man/converterr",
  "authors": [
    {
      "name": "Sergey Kanashin",
      "email": "goujon@mail.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "7.*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Converter\\Core\\": "src",
      "Converter\\Tools\\": "src",
      "Converter\\Demo\\": "src",
      "Converter\\Init\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Converter\\Unit\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "6.*"
  },
  "config": {
    "optimize-autoloader": true,
    "process-timeout": 5000,
    "sort-packages": true
  }
}
